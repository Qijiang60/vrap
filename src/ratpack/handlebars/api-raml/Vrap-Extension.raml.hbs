#%RAML 1.0 Extension
usage: Adds vrap specific headers
extends: {{fileName}}?{{queryParams}}
baseUri: {{proxyUri}}/api
{{#each resourceExtensions}}
{{indent}}{{resourcePath}}:
{{indent}}{{#each methods}}
{{indent}}    {{method}}:
{{indent}}       headers:
{{indent}}           X-Vrap-Mode?:
{{indent}}               displayName: Vrap mode
{{indent}}               enum: [ {{modes}} ]
{{/each}}
{{/each}}
